{
  "name": "chair-support",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "concurrently": "^6.0.2",
    "electron-is-dev": "^2.0.0",
    "jquery": "^3.6.0",
    "node": "^15.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.3",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
